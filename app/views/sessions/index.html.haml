#sign-in

  %header.header
    .header-content
      %h1
        = _('Sign in to continue')
      %p
        = _('You can use your Angel List account so we can sync with existing startups you have access to and help you win some time.')

  .content
    %a{:href => oauth2_sign_in_path(:provider => :angel_list), :class => 'button'}
      = _('Sign in with Angel List')
